{
  "name": "boilp",
  "version": "0.0.1",
  "description": "Creating multiple pages of web applications on the fly.",
  "main": "index.js",
  "scripts": {
    "backend:dev": "nodemon --ignore public/ --ignore app/ index.js",
    "backend:build": "node index.js",
    "frontend:dev": "webpack-dev-server --config configs/webpack.dev.js",
    "frontend:build": "webpack --config configs/webpack.build.js",
    "start": "concurrently --kill-others \"npm run backend:dev\" \"npm run frontend:dev\"",
    "build": "npm run frontend:build && npm run backend:build",
    "build:vercel": "npm run backend:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuquangpham/boilp.git"
  },
  "keywords": [
    "expressjs",
    "boilerplate",
    "nodejs"
  ],
  "author": "Vu Pham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuquangpham/boilp/issues"
  },
  "homepage": "https://github.com/vuquangpham/boilp#readme",
  "devDependencies": {
    "nodemon": "^2.0.20",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "@viivue/atomic-css": "^1.1.10",
    "babel-loader": "^8.2.5",
    "body-parser": "^1.20.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "gsap": "^3.11.4",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "ip": "^2.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^9.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "sass": "^1.64.1",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  }
}
